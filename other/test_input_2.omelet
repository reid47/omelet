def link_to url text = (a href={url} :: {text})

def youtube video_id width height =
    (iframe width={width} height={height}
      src={video_id | prepend "https://www.youtube.com/embed/" | append "?rel=0&amp;showinfo=0" }
      frameborder="0"
      allowfullscreen="allowfullscreen" ::)

