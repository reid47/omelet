{
  "name": "omelet",
  "version": "1.0.0",
  "description": "A language for writing web pages & templates that compiles into HTML",
  "dependencies": {
    "commander": "^2.9.0",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "striptags": "^2.1.1",
    "chokidar": "^1.4.2",
    "uglify-js": "^2.6.4"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "coveralls": "^2.11.11",
    "eslint": "^2.12.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-standard": "^1.3.2",
    "glob": "^7.0.6",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "scripts": {
    "coverage": "mocha --require blanket -R html-cov > coverage.html",
    "test": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 ./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint --quiet **/*.js"
  },
  "bin": {
    "omelet": "bin/command-line.js"
  },
  "author": "Reid Mitchell",
  "license": "MIT",
  "directories": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reid47/omelet.git"
  },
  "keywords": [
    "parser",
    "omelet",
    "template",
    "engine",
    "language"
  ],
  "bugs": {
    "url": "https://github.com/reid47/omelet/issues"
  },
  "homepage": "https://github.com/reid47/omelet#readme",
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  }
}
